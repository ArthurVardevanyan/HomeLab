{
  "$schema": "docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchDatasources": ["github-releases", "github-tags"],
      "matchPaths": ["ansible/**"],
      "groupName": "GitHub Releases"
    }
  ],
  "regexManagers": [
    {
      "fileMatch": ["^ansible/.+\\.yaml$"],
      "matchStrings": [
        "github.com/(?<owner>[^/]+)/(?<repo>[^/]+)/releases/download/(?<currentValue>v[0-9.]+)/(?<repo2>[^_]+)_(?<currentValue2>[0-9.]+)_linux_amd64"
      ],
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "{{{owner}}}/{{{repo}}}",
      "autoReplaceStringTemplate": "github.com/{{{owner}}}/{{{repo}}}/releases/download/v{{{newValue}}}/{{{repo}}}_v{{{newValue}}}_linux_amd64"
    },
    {
      "fileMatch": ["^ansible/.+\\.yaml$"],
